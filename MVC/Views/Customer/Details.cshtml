@model MVC.Models.Customers
@{

    ViewBag.Title = "Details";
    Layout = "~/Views/Shared/_Layout.cshtml";
 }

<h2>@Model.Name</h2>
<ul>
    <li>MembershipType: @Model.MembershipType.Name</li>
    @if (@Model.DateOfBirth.HasValue)
    {
    <li>Date Of Birth: @Model.DateOfBirth.Value.ToShortDateString()</li>
    }
</ul>
